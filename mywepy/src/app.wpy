<script>
import wepy from 'wepy'
import 'wepy-async-function'

import { setStore } from 'wepy-redux'
import configStore from './store'

const store = configStore()
setStore(store)

export default class extends wepy.app {
  config = {
    pages: [
      'pages/compile',
      'pages/index',
      'pages/my_tutor',
      'pages/tutor',
      'pages/my',
      'pages/quiz',
      'pages/my_question',
      'pages/quest_detail',
      'pages/apply',
      'pages/perhome'
    ],
    window: {
      backgroundTextStyle: 'light',
      navigationBarBackgroundColor: '#3385FF',
      navigationBarTitleText: '惠答',
      navigationBarTextStyle: '#fff'
    },
    tabBar: {
      color: '#000',
      selectedColor: '#1296db',
      list: [
        {
          pagePath: 'pages/index',
          iconPath: 'images/attention.png',
          selectedIconPath: 'images/attention1.png',
          text: '动态'
        },
        {
          pagePath: 'pages/tutor',
          iconPath: 'images/tutor.png',
          selectedIconPath: 'images/tutor1.png',
          text: '导师'
        },
        {
          pagePath: 'pages/my',
          iconPath: 'images/my.png',
          selectedIconPath: 'images/my1.png',
          text: '我的'
        }
      ]
    }
  }
  globalData = {
    userInfo: null
  }

  constructor () {
    super()
    this.use('requestfix')
  }

  onLaunch() {
    this.testAsync()
  }

  sleep (s) {
    return new Promise((resolve, reject) => {
      setTimeout(() => {
        resolve('promise resolved')
      }, s * 1000)
    })
  }

  async testAsync () {
    const data = await this.sleep(3)
    console.log(data)
  }

  getUserInfo(cb) {
    const that = this
    if (this.globalData.userInfo) {
      return this.globalData.userInfo
    }
    wepy.getUserInfo({
      success (res) {
        that.globalData.userInfo = res.userInfo
        cb && cb(res.userInfo)
      }
    })
  }
}
</script>
